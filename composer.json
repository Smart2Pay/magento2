{
    "name": "smart2pay/magento2",
    "description": "Use Smart2Pay module to have access to 100+ payment methods.",
    "type": "magento2-module",
    "version": "4.0.2",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "smart2pay/sdk-php": "*",
        "ext-json": "*"
    },
    "extra": {
        "map": [
            [
                "*",
                "Smart2Pay/GlobalPay"
            ]
        ]
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Smart2Pay\\GlobalPay\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smart2Pay\\GlobalPay\\": ""
        }
    }
}
